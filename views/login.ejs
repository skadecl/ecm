	<script src="../javascripts/jquery/jquery-1.11.3.min.js"></script>
	<script src="../javascripts/datatables/media/js/jquery.dataTables.min.js"></script>

	<!-- Angular App -->
	<script src="../javascripts/angular/angular.min.js"></script>
	<script src="../javascripts/angular/angular-route.min.js"></script>
	<script src="../javascripts/angular/angular-animate.min.js"></script>
	<script src="../javascripts/angular/angular-sanitize.min.js"></script>
	<script src="../javascripts/angular/angular-base64-upload.js"></script>
	<script src="../javascripts/angular/angular-simple-logger.min.js"></script>
	<script src="../javascripts/angular/angular-confirm/angular-confirm.min.js"></script>
	<script src="../javascripts/angular/ui-bootstrap-tpls-1.3.3.min.js"></script>
	<script src="../javascripts/angular/angular-datatables.min.js"></script>

	<script src="../javascripts/app/app.js"></script>
	<script src="../javascripts/app/app.controllers.js"></script>
	<script src="../javascripts/app/app.routes.js"></script>
	<script src="../javascripts/app/app.services.js"></script>

	<script src="../javascripts/app/services/session_service.js"></script>
	<script src="../javascripts/app/services/auth_service.js"></script>
